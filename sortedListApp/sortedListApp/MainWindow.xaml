        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:sortedListApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="txtKey" HorizontalAlignment="Left" Height="57" Margin="82,124,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="318"/>
        <TextBox x:Name="txtValue" HorizontalAlignment="Left" Height="57" Margin="82,260,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="318"/>
        <Button x:Name="bttnAdd" Content="Add" HorizontalAlignment="Left" Height="65" Margin="503,180,0,0" VerticalAlignment="Top" Width="157" FontSize="14" FontWeight="Bold" Click="bttnAdd_Click"/>

    </Grid>
</Window>
